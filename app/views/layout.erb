<!DOCTYPE html>
<html lang="en">
<head>
    <!-- removes cross-browser differences -->
  <link rel="stylesheet" href="/css/normalize.css">

    <!-- Styles go here plz -->
  <link rel="stylesheet" href="/css/application.css">

    <!-- Clean sans-serif combo -->
  <link href="https://fonts.googleapis.com/css?family=Roboto|Roboto+Condensed" rel="stylesheet">

    <!-- Slightly more snazy sans headers & serif content -->
<!--   <link href="https://fonts.googleapis.com/css?family=Merriweather|Montserrat" rel="stylesheet"> -->

    <!-- jQuery all the things -->
  <script
    src="https://code.jquery.com/jquery-3.2.1.min.js"
    integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="
    crossorigin="anonymous">
  </script>
  <script src="/js/application.js"></script>

  <title></title>
</head>

<body>
    <% if logged_in? %>
    <nav>
      <ul>
        <li><a href="/">home</a></li>  |
        <li><a href="/users/<%= current_user.id %>"><%= current_user.username %></a></li>  |
        <li><a><%= erb :"/sessions/_delete" %></a></li>
      </ul>
    </nav>
  <% else %>
    <nav>
      <ul>
        <li><a href="/">home</a></li>  |
        <li><a href="/sessions/new">login</a></li>  |
        <li><a href="/users/new">register</a></li>
      </ul>
    </nav>
  <% end %>
  <main class="container">
    <%= yield %>
  </main>
</body>
</html>


